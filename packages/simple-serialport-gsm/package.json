{
  "name": "simple-serialport-gsm",
  "version": "0.0.1",
  "scripts": {
    "build": "unbuild"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "dependencies": {
    "@simple-serialport-gsm/adapters": "workspace:*",
    "@simple-serialport-gsm/core": "workspace:*"
  },
  "devDependencies": {
    "@simple-serialport-gsm/types": "workspace:*"
  }
}
